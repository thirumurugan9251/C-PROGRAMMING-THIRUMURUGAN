#include <stdio.h>
int main() {
    int n;
    long fact = 1;
    printf("Enter a number: ");
    scanf("%d", &n);
    if (n < 0)
    {
        printf("Factorial is not defined for negative numbers.");
    }
    else 
    {
        printf("Factorial of %d = %lld", n, fact);
    }
    return 0;
}
